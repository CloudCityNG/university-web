!!! Strict
%html
  %head
    %title RMU
    = stylesheet_link_tag 'screen.css', :media => 'screen, projection'
    = stylesheet_link_tag 'print.css', :media => 'print'
    /[if IE]
      = stylesheet_link_tag 'ie.css', :media => 'screen, projection'
      
    = javascript_include_tag 'jquery-1.4.2.js', 'rails'
    = csrf_meta_tag
    
    = yield(:header_bottom)

  %body  
    #container
      #content
        #header= yield(:header)
        #nav
      
        = yield